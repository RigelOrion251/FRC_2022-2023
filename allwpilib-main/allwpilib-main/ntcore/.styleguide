cHeaderFileInclude {
  _c\.h$
}

cppHeaderFileInclude {
  (?<!_c)\.h$
  \.inc$
  \.inl$
}

cppSrcFileInclude {
  \.cpp$
}

generatedFileExclude {
  ntcore/doc/
}

repoRootNameOverride {
  ntcore
}

includeGuardRoots {
  ntcore/src/main/native/cpp/
  ntcore/src/main/native/include/
  ntcore/src/test/native/cpp/
}

includeOtherLibs {
  ^fmt/
  ^support/
  ^wpi/
  ^wpinet/
}
